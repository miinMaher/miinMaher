#include <bits/stdc++.h>
#include<iostream>
#include<string>
using namespace std;

  int main(){
    char y;
  string userName;
  cin >> userName;
  int x = userName.length();
 int DistinctCharacters = x;
  for( int i = 0 ; i <= (x-1) ; i++){
    for( int j = i+1; j <= (x-1);j++){
    if( userName[i] == userName[j]){
      --DistinctCharacters;
      y = userName[i];
      if( userName[j] == y){
        break;
      }
    }
   }
  }

  if(  DistinctCharacters% 2 == 0){
    cout << "CHAT WITH HER!"; 
  }else{
   cout << "IGNORE HIM!";
  }
  return 0;
  }
